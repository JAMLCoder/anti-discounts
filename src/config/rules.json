{
	"*.linio.com.co/p/*": {
		"image": {
			"[itemprop=image]": [
				["getAttribute", "content"],
				["//", "https://"]
			]
		},
		"title": {
			"[itemprop=name]": ["innerText"],
			".product-name": ["innerText"]
		},
		"price": {
			".lowest-price > .price-main-md": ["innerText", ["\\$", ""], ["\\.", ""]]
		}
	}
}