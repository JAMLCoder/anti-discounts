{
  "name": "Anti-Discount",
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "version": "0.1.0",
  "author": "AlejoNext <alejonext@gmail.com>",
  "homepage_url": "https://github.com/alejonext/anti-discounts",
  "default_locale": "es",
  "applications": {
    "gecko": {
      "id": "shopping-testpilot@mozilla.org",
      "strict_min_version": "63.0"
    }
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab",
    "storage",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ],
  "icons": {
    "128": "assets/tag-128.png",
    "96": "assets/tag-96.png",
    "64": "assets/tag-64.png",
    "32": "assets/tag-32.png"
  },
  "background": {
    "scripts": ["js/background.bundle.js"]
  },
  "browser_action": {
    "default_icon": "assets/tag-32.png",
    "theme_icons": [{
      "light": "assets/tag-32-light.png",
      "dark": "assets/tag-32.png",
      "size": 32
    }, {
      "light": "assets/tag-64-light.png",
      "dark": "assets/tag-64.png",
      "size": 64
    }, {
      "light": "assets/tag-96-light.png",
      "dark": "assets/tag-96.png",
      "size": 96
    }, {
      "light": "assets/tag-128-light.png",
      "dark": "assets/tag-128.png",
      "size": 128
    }],
    "default_title": "Anti-Discount",
    "default_popup": "index.html"
  }
}